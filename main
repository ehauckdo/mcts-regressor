#! /usr/bin/env python

import sys
from Node import * 
from sympy import sympify, symbols
from sympy.parsing.sympy_parser import parse_expr

def main(args):
    n = Node()
    for i in range(5):
        n.iteration()
        print(n.visits, n.reward) 

if __name__ == '__main__':
    main(sys.argv[1:])
